<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<GridPane xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="org.warehouse.controller.Controller"
          prefHeight="400.0" prefWidth="600.0">
    <Button text="Create External Tables" GridPane.columnIndex="0" GridPane.rowIndex="0" fx:id="createExternalSchema"
            onAction="#createExternalSchema"/>
    <Button text="Drop External Tables" GridPane.columnIndex="0" GridPane.rowIndex="1" fx:id="dropExternalSchema"
            onAction="#dropExternalSchema"/>
</GridPane>
